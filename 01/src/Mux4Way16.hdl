/**
 * 4-way 16-bit multiplexor:
 * out = a if sel == 00
 *       b if sel == 01
 *       c if sel == 10
 *       d if sel == 11
 */

CHIP Mux4Way16 {
    IN a[16], b[16], c[16], d[16], sel[2];
    OUT out[16];

    PARTS:
    Mux16(a=a,b=b,sel=sel[0],out=m1);
    And(a=sel[0],b=sel[1], out=s1);
    Mux16(a=c,b=d,sel=s1,out=m2);
    Not(in=sel[0], out=nots0);
    And(a=nots0, b=sel[1], out=s2);
    Or(a=s1,b=s2, out=s3);
    Mux16(a=m1,b=m2,sel=s3,out=out);
}